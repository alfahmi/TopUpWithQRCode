<?xml version="1.0" encoding="utf-8"?>
<manifest
	xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alfahmi.topupwithcode"
	android:versionCode="1"
    android:versionName="1.0">
 
    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="21" />
	<uses-permission 
		android:name="android.permission.WRITE_SETTINGS" />
	<uses-permission
		android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	<uses-permission
		android:name="android.permission.CALL_PHONE" />
    
	<application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.alfahmi.topupwithcode.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
		<activity 
			android:label="@string/alfahmi__scan_activity"
			android:name="com.journeyapps.barcodescanner.CaptureActivity" />
		<activity
			android:label="@string/alfahmi__settings_activity"
			android:name="com.alfahmi.topupwithcode.SettingsActivity" />
		<activity
			android:label="@string/alfahmi__trx_list_activity"
			android:name="com.alfahmi.topupwithcode.PenjualanActivity" />
		<activity
			android:label="@string/alfahmi__return_stock_activity"
			android:name="com.alfahmi.topupwithcode.ReturnStockActivity" />
    </application>
</manifest>
